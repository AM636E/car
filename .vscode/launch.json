{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Remote Debug on Raspberry Pi",
            "type": "cppdbg",
            "request": "launch",
            "program": "/",
            "args": [],
            "stopAtEntry": true,
            "cwd": "/home/nazarevic/control.out",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "miDebuggerServerAddress": "raspberrypi:port",
            "targetArchitecture": "arm",
            "sourceFileMap": {
                "/home/nazarevic": "${workspaceFolder}"
            }
        }
    ]
}
